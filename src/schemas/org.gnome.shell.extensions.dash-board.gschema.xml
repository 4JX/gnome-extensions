<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

  <schema id="org.gnome.shell.extensions.dash-board"
    path="/org/gnome/shell/extensions/dash-board/">

    <!-- panel-button -->
    <key name="hide-panel-button" type="b">
      <default>false</default>
    </key>

    <key name="replace-activities-button" type="b">
      <default>false</default> 
    </key>

    <key name="panel-button-position" type="i">
      <default>0</default> 
    </key>

    <key name="panel-button-position-offset" type="i">
      <default>0</default> 
    </key>

    <key name="panel-button-label" type="s">
      <default>"Dash"</default>
    </key>

    <key name="panel-button-icon-path" type="s">
      <default>""</default>
    </key>

    <key name="panel-button-icon-hide" type="b">
      <default>true</default>
    </key>

    <!-- shortcut -->
    <key name="dash-board-shortcut" type="as">
      <default><![CDATA[['<Super>d']]]></default>
    </key>

    <!-- layout -->
    <key name="dash-layout" type="i">
      <default>1</default>
    </key>

    <!-- links -->
    <key name="link-names" type="as">
      <default>['reddit', 'youtube', 'github', 'gmail', 'canvas', 'ncore']</default>
    </key>

    <key name="link-urls" type="as">
      <default>['https://www.reddit.com/', 'https://www.youtube.com/', 'https://www.github.com/', 'https://www.gmail.com/', 'https://canvas.elte.hu/belepes/', 'https://ncore.pro/login.php']</default>
    </key>

  </schema>
</schemalist>
